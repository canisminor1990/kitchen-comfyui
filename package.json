{
  "name": "kitchen-comfyui",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/canisminor1990/kitchen-comfyui.git"
  },
  "author": "canisminor1990 <i@canisminor.cc>",
  "sideEffects": false,
  "scripts": {
    "build": "umi build",
    "dev": "umi dev",
    "dev:with-mock": "cross-env MOCK=true umi dev",
    "docs": "typedoc --options typedoc.json",
    "postinstall": "umi setup",
    "lint": "eslint \"{src,test,mock}/**/*.{js,jsx,ts,tsx}\" --fix",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"**/**\"",
    "release": "semantic-release",
    "setup": "umi setup",
    "start": "npm run dev",
    "stylelint": "stylelint \"src/**/*.{css,less,js,jsx,ts,tsx}\" --fix",
    "test": "npm run lint",
    "type-check": "tsc -p tsconfig-check.json"
  },
  "lint-staged": {
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@ant-design/pro-components": "^2.5.1",
    "@floating-ui/react": "^0.24.2",
    "@radix-ui/react-icons": "^1.3.0",
    "antd": "^5.5.1",
    "antd-style": "^3.3.0",
    "exifr": "^7.1.3",
    "react-json-view": "^1.21.3",
    "react-use-websocket": "^4.3.1",
    "reactflow": "^11.7.2",
    "umi": "^4.0.69",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17",
    "@types/styled-components": "^5.1.26",
    "@semantic-release/github": "^8.1.0",
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/uuid": "^9.0.1",
    "@umijs/lint": "^4.0.70",
    "ahooks": "^3.7.7",
    "babel-plugin-styled-components": "^2.1.3",
    "commitlint": "^17",
    "commitlint-config-gitmoji": "^2",
    "cross-env": "^7",
    "dayjs": "^1.11.8",
    "eslint": "^8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.2.1",
    "husky": "^8",
    "lint-staged": "^13.2.2",
    "lodash-es": "^4.17.21",
    "polished": "^4.2.2",
    "postcss-less": "^6.0.0",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2",
    "prettier-plugin-organize-imports": "^3",
    "prettier-plugin-packagejson": "^2",
    "query-string": "^8.1.0",
    "re-resizable": "^6.9.9",
    "react-layout-kit": "^1.7.0",
    "react-rnd": "^10.4.1",
    "semantic-release": "^21",
    "semantic-release-config-gitmoji": "^1",
    "styled-components": "^5.3.11",
    "stylelint": "^15.6.3",
    "stylelint-config-clean-order": "^5.0.1",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-order": "^6.0.3",
    "typedoc": "^0.24.8",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.0.4",
    "use-merge-value": "^1.2.0",
    "uuid": "^9.0.0"
  }
}
